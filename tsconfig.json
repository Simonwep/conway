{
    "compileOnSave": false,
    "compilerOptions": {
        "strict": true,
        "outDir": "./dist/",
        "skipLibCheck": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "lib": ["webworker", "esnext", "dom"],
        "module": "ESNext",
        "target": "ES2019",
        "jsx": "react",
        "jsxFactory": "h"
    }
}
